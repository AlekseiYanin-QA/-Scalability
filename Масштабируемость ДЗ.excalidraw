    {
      "id": "smtBQw_ndlamZmb_NS26j",
      "type": "text",
      "x": 185.22222222222223,
      "y": 1293.9999999999998,
      "width": 1453.0390625,
      "height": 1350,
      "angle": 0,
      "strokeColor": "#1e1e1e",
      "backgroundColor": "transparent",
      "fillStyle": "solid",
